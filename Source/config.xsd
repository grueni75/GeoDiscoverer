<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:gd="http://www.untouchableapps.de/GeoDiscoverer/GDXMLSchemaExtension"
  xmlns="http://www.untouchableapps.de/GeoDiscoverer/config/1/0"
  targetNamespace="http://www.untouchableapps.de/GeoDiscoverer/config/1/0"
  elementFormDefault="qualified">
  <xsd:annotation>
    <xsd:documentation>Geo Discoverer Config schema</xsd:documentation>
  </xsd:annotation>
  <xsd:element name="GDC">
    <xsd:annotation>
      <xsd:documentation>All preferences of Geo Discoverer.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="expertMode" type="xsd:boolean" default="0" gd:upgrade="restore">
          <xsd:annotation>
            <xsd:documentation>Shows all available preferences if ticked.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="General">
          <xsd:annotation>
            <xsd:documentation>Preferences that do not fit in any other category.</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="unitSystem" default="metric" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>System of unit to use for reporting length and speed.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                  <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="metric"/>
                    <xsd:enumeration value="imperial"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element name="fileAccessRetries" type="xsd:integer" default="10">
                <xsd:annotation>
                  <xsd:documentation>Number of times to retry a file access operation before aborting.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="fileAccessWaitTime" type="xsd:integer" default="100">
                <xsd:annotation>
                  <xsd:documentation>Wait time in microseconds after a file access operation has failed before retrying.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="maintenancePeriod" type="xsd:integer" default="60">
                <xsd:annotation>
                  <xsd:documentation>Time in seconds between maintenance operations (storing unsaved data, optimizing data structures, ...).</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="wakeLock" type="xsd:boolean" default="0" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Screen backlight is kept on if ticked.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="watchDisplayTimeout" type="xsd:integer" default="10000">
                <xsd:annotation>
                  <xsd:documentation>Timeout in milliseconds for the display if executed on a watch.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="backButtonTurnsScreenOff" type="xsd:boolean" default="0" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Turns off the screen if the back button is pressed (to save energy). This requires that the app acts as a device admin. The device admin permission has to be granted manually in the device settings.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="createTraceLog" type="xsd:boolean" default="0" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Writes trace messages into a file in the log directory if ticked. The trace file allows a replay of the session for debugging purposes.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="createMessageLog" type="xsd:boolean" default="0" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Writes debug messages into a file in the log directory if ticked.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="createMutexLog" type="xsd:boolean" default="0" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Writes mutex locking messages into a file in the log directory if ticked.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="mutexLogUpdateInterval" type="xsd:integer" default="10">
                <xsd:annotation>
                  <xsd:documentation>Distance in seconds between updates of the mutex log.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="writeConfigMinWaitTime" type="xsd:integer" default="1" >
                <xsd:annotation>
                  <xsd:documentation>Minimum distance in seconds between writes of the config store.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="replayPeriod" type="xsd:integer" default="50000" >
                <xsd:annotation>
                  <xsd:documentation>Time in microseconds to wait before executing the next command during replay.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="audioWakeupDelay" type="xsd:integer" default="2">
                <xsd:annotation>
                  <xsd:documentation>Time in seconds the audio device (e.g., bluetooth speaker) requires to wake up.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="audioWakeupDuration" type="xsd:integer" default="5">
                <xsd:annotation>
                  <xsd:documentation>Time in seconds the audio device (e.g., bluetooth speaker) stays awake.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>                            
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="HeartRateMonitor">
          <xsd:annotation>
            <xsd:documentation>Preferences related to the heart rate monitor.</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="bluetoothAddress" type="xsd:string" default="FD:64:F3:96:A3:8C" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Address of the bluetooth low energy device that transmits heart rate information.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="maxHeartRate" type="xsd:integer" default="190" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Maximum heart rate above which an alarm sound is played.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>              
              <xsd:element name="startHeartRateZoneTwo" type="xsd:integer" default="118" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Heart rate at which zone two starts.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>              
              <xsd:element name="startHeartRateZoneThree" type="xsd:integer" default="135" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Heart rate at which zone three starts.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>              
              <xsd:element name="startHeartRateZoneFour" type="xsd:integer" default="152" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Heart rate at which zone four starts.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>              
              <xsd:element name="minHeartRateZoneChangeTime" type="xsd:integer" default="10">
                <xsd:annotation>
                  <xsd:documentation>Minimum time the heart rate needs to stay in a new zone before it is indicated by a sound..</xsd:documentation>
                </xsd:annotation>
              </xsd:element>              
              <xsd:element name="volumeHeartRateZoneChange" type="xsd:integer" default="25">
                <xsd:annotation>
                  <xsd:documentation>Volume to use for the heart rate zone change sound. Valid range is 0 to 100.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>              
              <xsd:element name="connectionWarningPeriod" type="xsd:integer" default="360">
                <xsd:annotation>
                  <xsd:documentation>Time in seconds between two missing heart rate connection warnings.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>              
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="EBikeMonitor">
          <xsd:annotation>
            <xsd:documentation>Preferences related to the E-Bike status monitor.</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="bluetoothAddress" type="xsd:string" default="D8:A9:8B:B2:E8:83" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Address of the bluetooth low energy device that transmits E-Bike status information.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="connected" type="xsd:integer" default="0" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates if an E-bike is connected.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="batteryLevel" type="xsd:string" default="0">
                <xsd:annotation>
                  <xsd:documentation>Battery level in percent.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="powerLevel" type="xsd:string" default="0">
                <xsd:annotation>
                  <xsd:documentation>Power level in steps.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="engineTemperature" type="xsd:string" default="0">
                <xsd:annotation>
                  <xsd:documentation>Temperature within engine in degress of celcius.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="distanceElectric" type="xsd:string" default="0">
                <xsd:annotation>
                  <xsd:documentation>Distance in km the engine was on and supported the drive.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="distanceTotal" type="xsd:string" default="0">
                <xsd:annotation>
                  <xsd:documentation>Distance in km the engine was either on or in standby.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="speed" type="xsd:string" default="0">
                <xsd:annotation>
                  <xsd:documentation>Speed in km/h the bike has currently.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="GoogleBookmarksSync">
          <xsd:annotation>
            <xsd:documentation>Preferences related to the Google Maps bookmark synchronizer.</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="active" type="xsd:boolean" default="0" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Enables the synchronization of Google Maps bookmarks.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>              
              <xsd:element name="cookies" type="xsd:string" default="invalid" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Cookies used for authentication.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="placesApiKey" type="xsd:string" default="Please set your API key obtained via Google Developers Console" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>API key to use when communicating with Google's places API.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="group" type="xsd:string" default="Google Maps" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Name of the group to use for Google Maps Bookmarks.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>                    
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Cockpit">
          <xsd:annotation>
            <xsd:documentation>Preferences related to cockpit apps.</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="App">
                <xsd:annotation>
                  <xsd:documentation>List of supported cockpit apps.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="MetaWatch">
                      <xsd:annotation>
                        <xsd:documentation>Shows cockpit infos on a meta watch.</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="active" type="xsd:boolean" default="0" gd:upgrade="restore">
                            <xsd:annotation>
                              <xsd:documentation>Enables the meta watch cockpit app.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="Vibration">
                      <xsd:annotation>
                        <xsd:documentation>Vibrates if important navigation events occur (turns, off route).</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="active" type="xsd:boolean" default="1" gd:upgrade="restore">
                            <xsd:annotation>
                              <xsd:documentation>Enables the vibration cockpit app.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="Voice">
                      <xsd:annotation>
                        <xsd:documentation>Speaks cockpit infos aloud.</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="active" type="xsd:boolean" default="1" gd:upgrade="restore">
                            <xsd:annotation>
                              <xsd:documentation>Enables the voice cockpit app.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="minDurationBetweenOffRouteAlerts" type="xsd:integer" default="55">
                            <xsd:annotation>
                              <xsd:documentation>Minimum duration between off route alerts in seconds.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="forwardTurnMaxAngle" type="xsd:double" default="10.0">
                            <xsd:annotation>
                              <xsd:documentation>Maximum angle a turn is detected as a forward drive.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="speakDelay" type="xsd:integer" default="6">
                            <xsd:annotation>
                              <xsd:documentation>Average delay in seconds the text-to-speech engine requires for preparing the voice.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="Dashboard">
                      <xsd:annotation>
                        <xsd:documentation>Sends cockpit infos to a device in the network that runs the Geo Dashboard app.</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="active" type="xsd:boolean" default="0" gd:upgrade="restore">
                            <xsd:annotation>
                              <xsd:documentation>Enables the network cockpit app.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="dotsPerInch" type="xsd:integer" default="320" gd:upgrade="restore">
                            <xsd:annotation>
                              <xsd:documentation>Density to use when drawing the dashboard.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="whiteBackground" type="xsd:integer" default="1" gd:upgrade="restore">
                            <xsd:annotation>
                              <xsd:documentation>Decides if a black or white background is used when drawing the dashboard.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="port" type="xsd:integer" default="11111" gd:upgrade="restore">
                            <xsd:annotation>
                              <xsd:documentation>Network port to use when communicating with the dashboard app.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="socketTimeout" type="xsd:integer" default="3">
                            <xsd:annotation>
                              <xsd:documentation>Time in seconds to wait before aborting communication via a socket.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>                          
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>  
              <xsd:element name="minUpdatePeriodNormal" type="xsd:integer" default="3000">
                <xsd:annotation>
                  <xsd:documentation>Minimum time in milliseconds between app updates if no turn is shown.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="offRouteAlertFastPeriod" type="xsd:integer" default="10000">
                <xsd:annotation>
                  <xsd:documentation>Time in milliseconds between vibrate notification if route has been just left.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="offRouteAlertFastCount" type="xsd:integer" default="6">
                <xsd:annotation>
                  <xsd:documentation>Number of vibrates to perform with fast period before switching to slow period.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="offRouteAlertSlowPeriod" type="xsd:integer" default="60000">
                <xsd:annotation>
                  <xsd:documentation>Time in milliseconds between vibrate notification if route has already been left for some time.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="minUpdatePeriodTurn" type="xsd:integer" default="1000">
                <xsd:annotation>
                  <xsd:documentation>Minimum time in milliseconds between app updates if a turn is shown.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="waitTimeBeforeAlert" type="xsd:integer" default="4000">
                <xsd:annotation>
                  <xsd:documentation>Time in milliseconds to wait before vibrating to ensure that app is updated.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Graphic">
          <xsd:annotation>
            <xsd:documentation>Preferences related to the graphic engine.</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Font">
                <xsd:annotation>
                  <xsd:documentation>Preferences related to the drawing of text.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="backgroundStrokeWidth" type="xsd:integer" default="2">
                      <xsd:annotation>
                        <xsd:documentation>Width of the stroke behind font letters (12 pt) in pixel for better contrast on black background.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="stringCacheSize" type="xsd:integer" default="64">
                      <xsd:annotation>
                        <xsd:documentation>Maximum size of the string map cache.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="fadeOutOffset" type="xsd:integer" default="32">
                      <xsd:annotation>
                        <xsd:documentation>Distance in pixel to the right border when to start fading out the character.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="sansFilename" type="xsd:string" default="Roboto-Regular.ttf">
                      <xsd:annotation>
                        <xsd:documentation>File to use as the sans font.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="sansBoldFilename" type="xsd:string" default="Roboto-Bold.ttf">
                      <xsd:annotation>
                        <xsd:documentation>File to use as the bold sans font.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="sansLargeSize" type="xsd:integer" default="16">
                      <xsd:annotation>
                        <xsd:documentation>Large size of the sans font.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="sansNormalSize" type="xsd:integer" default="12">
                      <xsd:annotation>
                        <xsd:documentation>Normal size of the sans font.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="sansSmallSize" type="xsd:integer" default="10">
                      <xsd:annotation>
                        <xsd:documentation>Small size of the sans font.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="sansTinySize" type="xsd:integer" default="8">
                      <xsd:annotation>
                        <xsd:documentation>Tiny size of the sans font.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="Widget">
                <xsd:annotation>
                  <xsd:documentation>Preferences related to graphic elements that are displayed above the map.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="SelectedColor">
                      <xsd:annotation>
                        <xsd:documentation>Color of the selected widget.</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="red" type="xsd:integer" default="255">
                            <xsd:annotation>
                              <xsd:documentation>Red component [0-255]</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="green" type="xsd:integer" default="127">
                            <xsd:annotation>
                              <xsd:documentation>Green component [0-255]</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="blue" type="xsd:integer" default="0">
                            <xsd:annotation>
                              <xsd:documentation>Blue component [0-255]</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="alpha" type="xsd:integer" default="255">
                            <xsd:annotation>
                              <xsd:documentation>Alpha component [0-255]</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="buttonRepeatDelay" type="xsd:integer" default="500000">
                      <xsd:annotation>
                        <xsd:documentation>Time in microseconds to wait before dispatching repeating commands.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="buttonLongPressDelay" type="xsd:integer" default="500000">
                      <xsd:annotation>
                        <xsd:documentation>Time in microseconds to wait before dispatching long press command.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="buttonRepeatPeriod" type="xsd:integer" default="10000">
                      <xsd:annotation>
                        <xsd:documentation>Time distance in microseconds between command dispatching.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="contextMenuDelay" type="xsd:integer" default="1000000">
                      <xsd:annotation>
                        <xsd:documentation>Time distance in microseconds before the context menu is shown.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="contextMenuAllowedPixelJitter" type="xsd:integer" default="2">
                      <xsd:annotation>
                        <xsd:documentation>Allowed pixel jitter when checking if a context menu shall be displayed.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="changePageDuration" type="xsd:integer" default="500000">
                      <xsd:annotation>
                        <xsd:documentation>Time in microseconds that the transition from the current page to the next page takes.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="widgetsActiveTimeout" type="xsd:integer" default="10000000">
                      <xsd:annotation>
                        <xsd:documentation>Time in microseconds to show the widgets after last interaction.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="changePageOvershoot" type="xsd:double" default="10.0">
                      <xsd:annotation>
                        <xsd:documentation>Distance in percent of the screen width that the page change shall overshoot.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="hiddenAnimationDuration" type="xsd:integer" default="500000">
                      <xsd:annotation>
                        <xsd:documentation>Time duration in microseconds of the translate animation if a widget is outside the screen.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>                    
                    <xsd:element name="maxPathDistance" type="xsd:double" default="0.07">
                      <xsd:annotation>
                        <xsd:documentation>Distance in inches on the screen a nearby path can be away max to show it to the user.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="FingerMenu">
                      <xsd:annotation>
                        <xsd:documentation>Color of the accuracy circle background around the location indicator.</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="enable" type="xsd:boolean" default="1" gd:upgrade="restore">
                            <xsd:annotation>
                              <xsd:documentation>Indicates if finger menu is enabled.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="radiusCircle" type="xsd:double" default="0.5">
                            <xsd:annotation>
                              <xsd:documentation>Radius in inch of the buttons around the finger in the circle.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="distanceRow" type="xsd:double" default="0.4">
                            <xsd:annotation>
                              <xsd:documentation>Distance in inch between the buttons in the row.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="angleOffset" type="xsd:double" default="45">
                            <xsd:annotation>
                              <xsd:documentation>Angle in degree to start placing widgets from the south of the menu.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="rowOffsetY" type="xsd:double" default="-0.94">
                            <xsd:annotation>
                              <xsd:documentation>Vertical offset of the button row.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="cursorInfoClosedOffsetY" type="xsd:double" default="-0.2">
                            <xsd:annotation>
                              <xsd:documentation>Vertical offset of the cursor info if finger menu is closed.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="cursorInfoOpenedOffsetY" type="xsd:double" default="-0.67">
                            <xsd:annotation>
                              <xsd:documentation>Vertical offset of the cursor info if finger menu is opened.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="animationDuration" type="xsd:integer" default="250000">
                            <xsd:annotation>
                              <xsd:documentation>Time in microseconds that a finger menu change animation lasts.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="closeTimeout" type="xsd:integer" default="10000000">
                            <xsd:annotation>
                              <xsd:documentation>Timeout in microseconds after the finger menu closes if not touched.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>                          
                    <xsd:element name="Device" maxOccurs="unbounded">
                      <xsd:annotation>
                        <xsd:documentation>Collection of pages that are displayed on the given device.</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>                        
                          <xsd:element name="selectedPage" type="xsd:string" default="Default" gd:upgrade="restore">
                            <xsd:annotation>
                              <xsd:documentation>Name of the currently selected widget page.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="Page" maxOccurs="unbounded">
                            <xsd:annotation>
                              <xsd:documentation>Collection of widgets that are displayed together.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Primitive" maxOccurs="unbounded">
                                  <xsd:annotation>
                                    <xsd:documentation>Defines a widget.</xsd:documentation>
                                  </xsd:annotation>
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name="type" default="button">
                                        <xsd:annotation>
                                          <xsd:documentation>Kind of widget.</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:simpleType>
                                          <xsd:restriction base="xsd:string">
                                            <xsd:enumeration value="button"/>
                                            <xsd:enumeration value="checkbox"/>
                                            <xsd:enumeration value="meter"/>
                                            <xsd:enumeration value="scale"/>
                                            <xsd:enumeration value="status"/>
                                            <xsd:enumeration value="navigation"/>
                                            <xsd:enumeration value="pathInfo"/>                                      
                                          </xsd:restriction>
                                        </xsd:simpleType>
                                      </xsd:element>
                                      <xsd:element name="Position" maxOccurs="unbounded">
                                        <xsd:annotation>
                                          <xsd:documentation>Defines the position of the widget for a given screen diagonal.</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                          <xsd:sequence>
                                            <xsd:element name="Portrait">
                                              <xsd:annotation>
                                                <xsd:documentation>Position of the widget in portrait orientation.</xsd:documentation>
                                              </xsd:annotation>
                                              <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element name="x" type="xsd:double" default="0">
                                                    <xsd:annotation>
                                                      <xsd:documentation>X coordinate in percent.</xsd:documentation>
                                                    </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="y" type="xsd:double" default="0">
                                                    <xsd:annotation>
                                                      <xsd:documentation>Y coordinate in percent.</xsd:documentation>
                                                    </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="xHidden" type="xsd:double" default="0">
                                                    <xsd:annotation>
                                                      <xsd:documentation>X coordinate in percent when widget is outside screen.</xsd:documentation>
                                                    </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="yHidden" type="xsd:double" default="0">
                                                    <xsd:annotation>
                                                      <xsd:documentation>Y coordinate in percent when widget is outside screen.</xsd:documentation>
                                                    </xsd:annotation>
                                                  </xsd:element>			                                            
                                                  <xsd:element name="z" type="xsd:double" default="0">
                                                    <xsd:annotation>
                                                      <xsd:documentation>Z coordinate in percent.</xsd:documentation>
                                                    </xsd:annotation>
                                                  </xsd:element>
                                                </xsd:sequence>
                                              </xsd:complexType>
                                            </xsd:element>
                                            <xsd:element name="Landscape">
                                              <xsd:annotation>
                                                <xsd:documentation>Position of the widget in landscape orientation.</xsd:documentation>
                                              </xsd:annotation>
                                              <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element name="x" type="xsd:double" default="0">
                                                    <xsd:annotation>
                                                      <xsd:documentation>X coordinate in percent.</xsd:documentation>
                                                    </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="y" type="xsd:double" default="0">
                                                    <xsd:annotation>
                                                      <xsd:documentation>Y coordinate in percent.</xsd:documentation>
                                                    </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="xHidden" type="xsd:double" default="0">
                                                    <xsd:annotation>
                                                      <xsd:documentation>X coordinate in percent when widget is outside screen.</xsd:documentation>
                                                    </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="yHidden" type="xsd:double" default="0">
                                                    <xsd:annotation>
                                                      <xsd:documentation>Y coordinate in percent when widget is outside screen.</xsd:documentation>
                                                    </xsd:annotation>
                                                  </xsd:element>	
                                                  <xsd:element name="z" type="xsd:double" default="0">
                                                    <xsd:annotation>
                                                      <xsd:documentation>Z coordinate in percent.</xsd:documentation>
                                                    </xsd:annotation>
                                                  </xsd:element>
                                                </xsd:sequence>
                                              </xsd:complexType>
                                            </xsd:element>
                                          </xsd:sequence>
                                          <xsd:attribute name="refScreenDiagonal">
                                            <xsd:annotation>
                                              <xsd:documentation>Screen diagonal in inch this widget position was designed for.</xsd:documentation>
                                            </xsd:annotation>
                                          </xsd:attribute>
                                        </xsd:complexType>
                                      </xsd:element>
                                      <xsd:element name="ActiveColor">
                                        <xsd:annotation>
                                          <xsd:documentation>Color of the widget if it is activated.</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                          <xsd:sequence>
                                            <xsd:element name="red" type="xsd:integer" default="255">
                                              <xsd:annotation>
                                                <xsd:documentation>Red component [0-255]</xsd:documentation>
                                              </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="green" type="xsd:integer" default="255">
                                              <xsd:annotation>
                                                <xsd:documentation>Green component [0-255]</xsd:documentation>
                                              </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="blue" type="xsd:integer" default="255">
                                              <xsd:annotation>
                                                <xsd:documentation>Blue component [0-255]</xsd:documentation>
                                              </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="alpha" type="xsd:integer" default="255">
                                              <xsd:annotation>
                                                <xsd:documentation>Alpha component [0-255]</xsd:documentation>
                                              </xsd:annotation>
                                            </xsd:element>
                                          </xsd:sequence>
                                        </xsd:complexType>
                                      </xsd:element>
                                      <xsd:element name="InactiveColor">
                                        <xsd:annotation>
                                          <xsd:documentation>Color of the widdget if it is not activated.</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                          <xsd:sequence>
                                            <xsd:element name="red" type="xsd:integer" default="255">
                                              <xsd:annotation>
                                                <xsd:documentation>Red component [0-255]</xsd:documentation>
                                              </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="green" type="xsd:integer" default="255">
                                              <xsd:annotation>
                                                <xsd:documentation>Green component [0-255]</xsd:documentation>
                                              </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="blue" type="xsd:integer" default="255">
                                              <xsd:annotation>
                                                <xsd:documentation>Blue component [0-255]</xsd:documentation>
                                              </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="alpha" type="xsd:integer" default="100">
                                              <xsd:annotation>
                                                <xsd:documentation>Alpha component [0-255]</xsd:documentation>
                                              </xsd:annotation>
                                            </xsd:element>
                                          </xsd:sequence>
                                        </xsd:complexType>
                                      </xsd:element>
                                      <xsd:element name="BusyColor">
                                        <xsd:annotation>
                                          <xsd:documentation>Color of the widget if it is busy.</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                          <xsd:sequence>
                                            <xsd:element name="red" type="xsd:integer" default="255">
                                              <xsd:annotation>
                                                <xsd:documentation>Red component [0-255]</xsd:documentation>
                                              </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="green" type="xsd:integer" default="255">
                                              <xsd:annotation>
                                                <xsd:documentation>Green component [0-255]</xsd:documentation>
                                              </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="blue" type="xsd:integer" default="255">
                                              <xsd:annotation>
                                                <xsd:documentation>Blue component [0-255]</xsd:documentation>
                                              </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="alpha" type="xsd:integer" default="255">
                                              <xsd:annotation>
                                                <xsd:documentation>Alpha component [0-255]</xsd:documentation>
                                              </xsd:annotation>
                                            </xsd:element>
                                          </xsd:sequence>
                                        </xsd:complexType>
                                      </xsd:element>
                                      <xsd:element name="SafetyColor">
                                        <xsd:annotation>
                                          <xsd:documentation>Color of the widget if it is in safety state.</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                          <xsd:sequence>
                                            <xsd:element name="red" type="xsd:integer" default="255">
                                              <xsd:annotation>
                                                <xsd:documentation>Red component [0-255]</xsd:documentation>
                                              </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="green" type="xsd:integer" default="255">
                                              <xsd:annotation>
                                                <xsd:documentation>Green component [0-255]</xsd:documentation>
                                              </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="blue" type="xsd:integer" default="255">
                                              <xsd:annotation>
                                                <xsd:documentation>Blue component [0-255]</xsd:documentation>
                                              </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="alpha" type="xsd:integer" default="255">
                                              <xsd:annotation>
                                                <xsd:documentation>Alpha component [0-255]</xsd:documentation>
                                              </xsd:annotation>
                                            </xsd:element>
                                          </xsd:sequence>
                                        </xsd:complexType>
                                      </xsd:element>
                                      <xsd:element name="iconFilename" type="xsd:string" default="unknown" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Filename of the background image.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="command" type="xsd:string" default="unknown" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Command to execute if widget is clicked.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="longPressCommand" type="xsd:string" default="unknown" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Command to execute if widget is pressed for some time.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="safetyStepDuration" type="xsd:integer" default="1">
                                        <xsd:annotation>
                                          <xsd:documentation>Duratio of the safety step in microseconds.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>                  
                                      <xsd:element name="repeat" type="xsd:integer" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Executes command repeatedly if set.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="checkedIconFilename" type="xsd:string" default="unknown" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Filename of the background image showing the checked state.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="checkedCommand" type="xsd:string" default="unknown" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Command to execute if widget is checked.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="uncheckedIconFilename" type="xsd:string" default="unknown" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Filename of the background image showing the unchecked state.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="uncheckedCommand" type="xsd:string" default="unknown" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Command to execute if widget is unchecked.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="stateConfigPath" type="xsd:string" default="unknown" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Path to the config entry that holds the check state.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="stateConfigName" type="xsd:string" default="unknown" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Name of the config entry that holds the check state.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="meterType" default="altitude" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Kind of meter to display.</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:simpleType>
                                          <xsd:restriction base="xsd:string">
                                            <xsd:enumeration value="altitude"/>
                                            <xsd:enumeration value="speed"/>
                                            <xsd:enumeration value="trackLength"/>
                                          </xsd:restriction>
                                        </xsd:simpleType>
                                      </xsd:element>
                                      <xsd:element name="updateInterval" type="xsd:integer" default="1000000" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Time in microseconds to elapse before updating the widget.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="labelY" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Vertical position of the label in percent of the widget height.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="valueY" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Vertical position of the value in percent of the widget height.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="unitY" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Vertical position of the unit in percent of the widget height.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="tickLabelOffsetX" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Horizontal offset for the scale values in percent of the widget width.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="mapLabelOffsetY" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Vertical offset for the map label in percent of the widget height.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="layerLabelOffsetY" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Vertical offset for the layer label in percent of the widget height.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="labelWidth" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Width of the text in percent of the widget width.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="width" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Width of the widget in inches.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="height" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Height of the widget in inches.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="textRowFirstOffsetY" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Vertical offset for the first row in percent of the widget height.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>                                
                                      <xsd:element name="textRowSecondOffsetY" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Vertical offset for the second row in percent of the widget height.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>   
                                      <xsd:element name="textRowThirdOffsetY" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Vertical offset for the third row in percent of the widget height.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>                                
                                      <xsd:element name="textRowFourthOffsetY" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Vertical offset for the fourth row in percent of the widget height.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>   
                                      <xsd:element name="textColumnOffsetX" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Horizontal offset for each column in percent of the widget width.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>   
                                      <xsd:element name="textColumnCount" type="xsd:integer" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Number of text columns to use.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>   
                                      <xsd:element name="clockOffsetY" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Vertical offset for the clock text in percent of the widget height.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>   
                                      <xsd:element name="turnDistanceValueOffsetY" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Vertical offset for the turn distance value in percent of the widget height.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>   
                                      <xsd:element name="directionIconFilename" type="xsd:string" default="unknown" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Filename of the image showing the direction of travel.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="arrowIconFilename" type="xsd:string" default="unknown" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Filename of the image showing the arrow.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="targetIconFilename" type="xsd:string" default="unknown" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Filename of the image showing the target.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="separatorIconFilename" type="xsd:string" default="unknown" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Filename of the image showing the separator.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="blindIconFilename" type="xsd:string" default="unknown" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Filename of the image showing the background if below content shall be defocused.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="statusIconFilename" type="xsd:string" default="unknown" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Filename of the image showing the status fields.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="batteryIconChargingFilename" type="xsd:string" default="unknown" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Filename of the image showing the a battery dot that is charging.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="batteryIconFullFilename" type="xsd:string" default="unknown" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Filename of the image showing the a battery dot that is full.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="batteryIconEmptyFilename" type="xsd:string" default="unknown" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Filename of the image showing the a battery dot that is empty.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="directionChangeDuration" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Duration in microseconds of a direction change.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="targetRadius" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Radius of the target in percent.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="orientationLabelRadius" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Radius of the orientation labels in percent.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="batteryIconRadius" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Radius of the battery icons in percent.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="turnLineWidth" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Line width of the turn indicator.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="turnLineArrowOverhang" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Arrow overhang of the turn indicator.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="turnLineArrowHeight" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Arrow height of the turn indicator.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="turnLineStartHeight" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Start height of the turn indicator.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="turnLineMiddleHeight" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Middle height of the turn indicator.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="turnLineStartX" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Start x position of the turn arrow.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="turnLineStartY" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Start y position of the turn arrow.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="turnWatchAlpha" type="xsd:integer" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Color alpha value to apply when displayed on a watch.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>			                                
                                      <xsd:element name="minTouchDetectionRadius" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Min radius in percent of the widget height a touch has to have from the center to be registered as a hit on the widget.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="circularButtonAngle" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Opening angle in degress which a touch is detected as a hit of a button within the widget.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="statusTextWidthLimit" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Maximum width in percent of the widget width a status text on a watch shall have.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>			                                
                                      <xsd:element name="statusTextRadius" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Radius in percent of the widget width a status text on a watch shall have.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>	
                                      <xsd:element name="clockRadius" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Radius in percent of the widget width the clock on a watch shall have.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>	
                                      <xsd:element name="statusTextAngleOffset" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Angle offset in degrees a status text on a watch shall have.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>	
                                      <xsd:element name="pathNameOffsetX" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Horizontal offset of the path name in percent of the widget width.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="pathNameOffsetY" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Vertical offset of the path name in percent of the widget height.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="pathNameWidth" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Width of the path name in percent of the widget width.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
                                      <xsd:element name="pathValuesWidth" type="xsd:double" default="0" minOccurs="0">
                                        <xsd:annotation>
                                          <xsd:documentation>Width of the path info values in percent of the widget width.</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:element>
