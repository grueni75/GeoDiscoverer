                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="centerIconFilename" type="xsd:string" default="center">
                <xsd:annotation>
                  <xsd:documentation>Image of the center cursor.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="pathStartFlagIconFilename" type="xsd:string" default="pathStartFlag">
                <xsd:annotation>
                  <xsd:documentation>Image of the path start flag.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="pathEndFlagIconFilename" type="xsd:string" default="pathEndFlag">
                <xsd:annotation>
                  <xsd:documentation>Image of the path end flag.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="locationIconFilename" type="xsd:string" default="location">
                <xsd:annotation>
                  <xsd:documentation>Image of the location cursor.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="targetIconFilename" type="xsd:string" default="target">
                <xsd:annotation>
                  <xsd:documentation>Image of the target cursor.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="navigationPointIconFilename" type="xsd:string" default="navigationPoint">
                <xsd:annotation>
                  <xsd:documentation>Filename of the image showing a navigation point.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="arrowIconFilename" type="xsd:string" default="arrow">
                <xsd:annotation>
                  <xsd:documentation>Image of the arrow indicating the direction to the target.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="pathDirectionIconFilename" type="xsd:string" default="pathDirection">
                <xsd:annotation>
                  <xsd:documentation>Image of the path arrow that indicates the direction.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="compassConeFilename" type="xsd:string" default="compassCone">
                <xsd:annotation>
                  <xsd:documentation>Image of the compass cone.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="tileImageNotCachedFilename" type="xsd:string" default="tileImageNotCached">
                <xsd:annotation>
                  <xsd:documentation>Image of a not yet cached tile.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="tileImageNotDownloadedFilename" type="xsd:string" default="tileImageNotDownloaded">
                <xsd:annotation>
                  <xsd:documentation>Image of a not yet downloaded tile.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="tileImageDownloadErrorOccuredFilename" type="xsd:string" default="tileImageDownloadErrorOccured">
                <xsd:annotation>
                  <xsd:documentation>Image of a tile that had download errors.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="pathMinSegmentLength" type="xsd:integer" default="8">
                <xsd:annotation>
                  <xsd:documentation>Minimum segment length of tracks, routes and other paths.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="pathMinDirectionDistance" type="xsd:integer" default="64">
                <xsd:annotation>
                  <xsd:documentation>Minimum distance between two direction arrows of tracks, routes and other paths.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="pathWidth" type="xsd:integer" default="8">
                <xsd:annotation>
                  <xsd:documentation>Width of tracks, routes and other paths.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="animationDuration" type="xsd:integer" default="500000">
                <xsd:annotation>
                  <xsd:documentation>Duration in microseconds of the naviation point appearance animation.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="lineNumberOfStrokesOtherSegemnts" type="xsd:integer" default="32">
                <xsd:annotation>
                  <xsd:documentation>Number of strokes to reserve if current segment has no strokes left anymore.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="rectangleListNumberOfRectanglesOtherSegemnts" type="xsd:integer" default="4">
                <xsd:annotation>
                  <xsd:documentation>Number of rectangles to reserve if current segment has no rectangles left anymore.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Map">
          <xsd:annotation>
            <xsd:documentation>Preferences related to the map engine.</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="folder" type="xsd:string" default="Default" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Folder that contains map images.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="neighborPixelTolerance" type="xsd:integer" default="2">
                <xsd:annotation>
                  <xsd:documentation>Maximum allowed difference in pixels to classify a tile as a neighbor.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="tileLength" type="xsd:integer" default="256">
                <xsd:annotation>
                  <xsd:documentation>Width and height of a tile in pixel.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="tileCacheSizeFactor" type="xsd:integer" default="2">
                <xsd:annotation>
                  <xsd:documentation>Determines the number of map tiles to cache. The maximum number of visible tiles is multiplied by this factor.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="initDistance" type="xsd:integer" default="65536">
                <xsd:annotation>
                  <xsd:documentation>Distance in pixel to the max/min values of integer that triggers a initialization of the map to avoid overflows.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="tileOffScreenFactor" type="xsd:double" default="2">
                <xsd:annotation>
                  <xsd:documentation>Factor to scale the screen length for rendering additional tiles outside the screen .</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="visibleTileLimit" type="xsd:integer" default="3">
                <xsd:annotation>
                  <xsd:documentation>Determines the maximum number of visible Tiles. The screen length in pixels is divided by the tile length. The result is multiplied by this factor.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="returnToLocationTimeout" type="xsd:integer" default="20000000">
                <xsd:annotation>
                  <xsd:documentation>Time in microseconds that must elapse before the map is repositioned to the current location.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="returnToLocation" type="xsd:boolean" default="1" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Indicates if the map shall be centered around the location if the returnToLocationTimeout has elapsed.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="zoomLevelLock" type="xsd:boolean" default="0" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Indicates if the zoom level of the map shall not be changed when zooming.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="averageMercatorTileSize" type="xsd:integer" default="128000">
                <xsd:annotation>
                  <xsd:documentation>Average size of a mercator tile gda file.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="visualizationMaxContainerCountPerRound" type="xsd:integer" default="10">
                <xsd:annotation>
                  <xsd:documentation>Number of map containers to visualize per map update round.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="LastPosition" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Information about the last shown position.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="folder" type="xsd:string" default="*unknown*" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Last used map folder.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="lng" type="xsd:double" default="0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Longitude of the last position in the map.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="lat" type="xsd:double" default="0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Latitude of the last position in the map.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="latScale" type="xsd:double" default="0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Latitude scale of the last position in the map.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="lngScale" type="xsd:double" default="0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Longitude scale of the last position in the map.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="zoom" type="xsd:double" default="1" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Zoom value used in the graphic engine when the last position was active.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="angle" type="xsd:double" default="0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Angle value used in the graphic engine when the last position was active.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="zoomLevel" type="xsd:integer" default="1" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Map zoom level used in the map engine when the last position was active.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="mapContainerCacheSize" type="xsd:integer" default="256">
                <xsd:annotation>
                  <xsd:documentation>Number of map containers to remember if the source is using a memory cache.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="mapArchiveCacheSize" type="xsd:integer" default="1024">
                <xsd:annotation>
                  <xsd:documentation>Number of map archives to store if the source is using a disk cache.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>              
              <xsd:element name="downloadErrorWaitTime" type="xsd:integer" default="1">
                <xsd:annotation>
                  <xsd:documentation>Time in seconds to wait after a download error before starting a new download.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="maxDownloadRetries" type="xsd:integer" default="360">
                <xsd:annotation>
                  <xsd:documentation>Maximum number of retries before a download is aborted.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="numerOfDownloadThreads" type="xsd:integer" default="8">
                <xsd:annotation>
                  <xsd:documentation>Number of threads to spawn that download images.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="downloadQueueRecommendedSize" type="xsd:integer" default="1000">
                <xsd:annotation>
                  <xsd:documentation>Maximum number of tiles that should be queued in the download queue.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="imageQueueMaxSize" type="xsd:integer" default="16">
                <xsd:annotation>
                  <xsd:documentation>Maximum number of images that should be queued for writing onto the storage.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="downloadAreaLength" type="xsd:integer" default="50" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Length of the square in kilometers that is downloaded around a route position.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>              
              <xsd:element name="downloadAreaMinDistance" type="xsd:integer" default="5">
                <xsd:annotation>
                  <xsd:documentation>Distance in kilometers that a route point must be away from the previous one before a new area is downloaded.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>              
              <xsd:element name="DownloadJob">
                <xsd:annotation>
                  <xsd:documentation>Defines a map area that shall be downloaded to the device storage.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="latNorth" type="xsd:double">
                      <xsd:annotation>
                        <xsd:documentation>Latitude of the north border in degrees.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="latSouth" type="xsd:double">
                      <xsd:annotation>
                        <xsd:documentation>Latitude of the south border in degrees.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="lngWest" type="xsd:double">
                      <xsd:annotation>
                        <xsd:documentation>Longitude of the west border in degrees.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="lngEast" type="xsd:double">
                      <xsd:annotation>
                        <xsd:documentation>Longitude of the east border in degrees.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="routeName" type="xsd:string" default="">
                      <xsd:annotation>
                        <xsd:documentation>Name of the route to download the map for.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="zoomLevels" type="xsd:string">
                      <xsd:annotation>
                        <xsd:documentation>Zoom level to download.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="estimateOnly" type="xsd:boolean">
                      <xsd:annotation>
                        <xsd:documentation>Estimate required disk usage only.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                  </xsd:sequence>
                  <xsd:attribute name="name">
                    <xsd:annotation>
                      <xsd:documentation>Name of the download job.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:attribute>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="Remote">
                <xsd:annotation>
                  <xsd:documentation>Information for receiving maps from a remote device (e.g., on a watch from a mobile phone).</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="reset" type="xsd:integer" default="0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Indicates that the cached tiles need to be reset.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="folder" type="xsd:string" default="Default" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Remote folder that contains map images.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="minZoomLevel" type="xsd:integer" default="0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Min zoom level of the remote map.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="maxZoomLevel" type="xsd:integer" default="0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Max zoom level of the remote map.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="centerLat" type="xsd:double" default="0.0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Latitude of the remote map center.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="centerLng" type="xsd:double" default="0.0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Longitude of the remote map center.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="centerLngScale" type="xsd:double" default="0.0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Latitude scale of the remote map center.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="centerLatScale" type="xsd:double" default="0.0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Longitude scale of the remote map center..</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>              
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Navigation">
          <xsd:annotation>
            <xsd:documentation>Preferences related to the navigation engine.</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="locationOutdatedThreshold" type="xsd:integer" default="120000">
                <xsd:annotation>
                  <xsd:documentation>Duration in milliseconds after which a position will be discarded.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="locationSignificantlyInaccurateThreshold" type="xsd:integer" default="100">
                <xsd:annotation>
                  <xsd:documentation>Distance in meter that indicates a significantly less accurate position.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="trackRecordingMinDistance" type="xsd:integer" default="25">
                <xsd:annotation>
                  <xsd:documentation>Required minimum distance in meter to the last track point such that the point is added to the track.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="TrackColor">
                <xsd:annotation>
                  <xsd:documentation>Color of tracks.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="red" type="xsd:integer" default="255">
                      <xsd:annotation>
                        <xsd:documentation>Red component [0-255]</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="green" type="xsd:integer" default="127">
                      <xsd:annotation>
                        <xsd:documentation>Green component [0-255]</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="blue" type="xsd:integer" default="0">
                      <xsd:annotation>
                        <xsd:documentation>Blue component [0-255]</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="alpha" type="xsd:integer" default="255">
                      <xsd:annotation>
                        <xsd:documentation>Alpha component [0-255]</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="recordTrack" type="xsd:boolean" default="0" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Indicates if track recording is enabled.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="pathInfoName" type="xsd:string" default="." gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Name of the path that is currently shown.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="pathInfoLocked" type="xsd:boolean" default="0" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Indicates if path info widget does not change the path if the map is moved.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="lastRecordedTrackFilename" type="xsd:string" default="unknown" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Filename of the last recorded track.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="averageTravelSpeed" type="xsd:double" default="4.167">
                <xsd:annotation>
                  <xsd:documentation>Average speed in meters per second for estimating travel duration.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="Target">
                <xsd:annotation>
                  <xsd:documentation>Point to navigate to.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="lng" type="xsd:double" default="0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Longitude in degrees.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="lat" type="xsd:double" default="0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Latitude in degrees.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="visible" type="xsd:boolean" default="0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Target is drawn if ticked.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="Route" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Defines a route for navigation.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="NormalColor">
                      <xsd:annotation>
                        <xsd:documentation>Highlight color of the route. If set, the route color will be periodically change between normal color and highlight color (blink mode).</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="red" type="xsd:integer" default="255">
                            <xsd:annotation>
                              <xsd:documentation>Red component [0-255]</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="green" type="xsd:integer" default="0">
                            <xsd:annotation>
                              <xsd:documentation>Green component [0-255]</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="blue" type="xsd:integer" default="255">
                            <xsd:annotation>
                              <xsd:documentation>Blue component [0-255]</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="alpha" type="xsd:integer" default="255">
                            <xsd:annotation>
                              <xsd:documentation>Alpha component [0-255]</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="HighlightColor">
                      <xsd:annotation>
                        <xsd:documentation>Normal color of the route.</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="red" type="xsd:integer" default="0">
                            <xsd:annotation>
                              <xsd:documentation>Red component [0-255]</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="green" type="xsd:integer" default="255">
                            <xsd:annotation>
                              <xsd:documentation>Green component [0-255]</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="blue" type="xsd:integer" default="255">
                            <xsd:annotation>
                              <xsd:documentation>Blue component [0-255]</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="alpha" type="xsd:integer" default="255">
                            <xsd:annotation>
                              <xsd:documentation>Alpha component [0-255]</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="reverse" type="xsd:boolean" default="0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Route is reversed if ticked.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="importWaypoints" type="xsd:boolean" default="0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Decides if waypoints from a route are imported (0=not decided yet, 1=yes, 2=no).</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="visible" type="xsd:boolean" default="1" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Route is drawn on map if ticked.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="startFlagIndex" type="xsd:integer" default="-1" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Position of the start flag.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="endFlagIndex" type="xsd:integer" default="-1" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Position of the end flag.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                  </xsd:sequence>
                  <xsd:attribute name="name">
                    <xsd:annotation>
                      <xsd:documentation>Filename of the route.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:attribute>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="activeRoute" type="xsd:string" default="none" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Name of the route to navigate along.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="minDistanceToRouteWayPoint" type="xsd:double" default="200">
                <xsd:annotation>
                  <xsd:documentation>Minimum distance in meters to a point on the route to consider it as the target for navigation.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="minDistanceToTurnWayPoint" type="xsd:double" default="10">
                <xsd:annotation>
                  <xsd:documentation>Minimum distance in meters to a turn up to it is indicated.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="maxDistanceToTurnWayPoint" type="xsd:double" default="150">
                <xsd:annotation>
                  <xsd:documentation>Maximum distance in meters to a turn before it is indicated.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="minTurnAngle" type="xsd:double" default="45">
                <xsd:annotation>
                  <xsd:documentation>Maximum angle at which a turn in the path is detected.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="turnDetectionDistance" type="xsd:double" default="25">
                <xsd:annotation>
                  <xsd:documentation>Distance in meters to look forward and back for detecting a turn.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="minDistanceToNavigationUpdate" type="xsd:double" default="5">
                <xsd:annotation>
                  <xsd:documentation>Minimum distance that need to be travelled before the navigation information is updated.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="minSpeedToSignalOffRoute" type="xsd:double" default="0.2">
                <xsd:annotation>
                  <xsd:documentation>Minimum speed required to indicate an off route situation.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="minDistanceToBeOffRoute" type="xsd:double" default="50">
                <xsd:annotation>
                  <xsd:documentation>Minimum distance from nearest route point such that navigation considers location to be off route.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="maxAltitudeFilterDistance" type="xsd:double" default="150">
                <xsd:annotation>
                  <xsd:documentation>Maximum distance in meters to still include a previous path point into altitude averaging.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="minAltitudeChange" type="xsd:double" default="5">
                <xsd:annotation>
                  <xsd:documentation>Minimum change of altitude required to update altitude meters.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="AddressPoint" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Defines an address.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="address" type="xsd:string" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>The address in text form.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="lng" type="xsd:double" default="0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Longitude of the address.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="lat" type="xsd:double" default="0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Latitude of the address.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="timestamp" type="xsd:long" default="0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Timestamp in seconds that the entry was last updated.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>              
                    <xsd:element name="foreignTimestamp" type="xsd:long" default="0" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Timestamp in seconds used in the system this entry was synchronized with.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>              
                    <xsd:element name="group" type="xsd:string" default="Default" gd:upgrade="restore">
                      <xsd:annotation>
                        <xsd:documentation>Name of the group the address belongs to.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>                    
                  </xsd:sequence>
                  <xsd:attribute name="name">
                    <xsd:annotation>
                      <xsd:documentation>Name of the address.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:attribute>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="selectedAddressPointGroup" type="xsd:string" default="Default" gd:upgrade="restore">
                <xsd:annotation>
                  <xsd:documentation>Name of the address point group that is currently displayed.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>     
              <xsd:element name="maxAddressPointAlarmDistance" type="xsd:double" default="1000">
                <xsd:annotation>
                  <xsd:documentation>Maximum distance in meters to an address point at which a user notification shall be generated.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>     
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="version">
        <xsd:annotation>
          <xsd:documentation>Config version.</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
